{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Schemes": {
      "Bearer": {
        "Authority": "https://yourdomain.auth0.com",
        "ValidAudiences": [ "https://your-api-id.com" ],
        "ValidIssuer": "yourdomain.auth0.com"
      }
    }
  }
}
